worker_processes 1;
daemon off; # important to run in the foreground

error_log /var/log/nginx/error.log;
pid /run/nginx.pid;

events {
    worker_connections 1024;
}

http {
    server {
        listen ${PORT};
        server_name localhost;

        location / {
            root /path/to/your/static/files;
            index index.html index.htm;
            try_files $uri $uri/ =404;
        }
    }
}
